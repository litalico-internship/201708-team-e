<%= form_with model: @experience do |form| %>
    <p>タイトル</p>
    <%= form.text_field :title %>

    <p>日付</p>
    <%= form.date_field :date %>

    <p>場所</p>
    <%= form.text_field :place %>

    <p>写真</p>
    <%= form.text_field :image %>

    <p>内容</p>
    <%= form.text_field :content %>
    <%= form.submit '作成' %>
<% end %>
